{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Database": {
    "ConnectionStringPattern": "Host=postgres;Database=TodoListDB;Username={0};Password={1}",
    "Username": ".env",
    "Password": ".env"
  },
  "JWT" : {
    "SecretKey": ".env",
    "Issuer": "TodoList.Backend",
    "Audience": "TodoList.Frontend",
    "AccessExpirationMinutes": 3
  }
}